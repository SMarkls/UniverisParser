<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net7.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <RootNamespace>UniverisParser.ParserLibrary</RootNamespace>
    </PropertyGroup>

    <ItemGroup>
      <EmbeddedResource Update="ParserResources.resx">
        <Generator>ResXFileCodeGenerator</Generator>
        <LastGenOutput>ParserResources.Designer.cs</LastGenOutput>
      </EmbeddedResource>
    </ItemGroup>

    <ItemGroup>
      <Compile Update="ParserResources.Designer.cs">
        <DesignTime>True</DesignTime>
        <AutoGen>True</AutoGen>
        <DependentUpon>ParserResources.resx</DependentUpon>
      </Compile>
    </ItemGroup>

</Project>
